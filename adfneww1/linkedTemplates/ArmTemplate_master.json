{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfneww1"
		},
		"LS_ADLS_eystrgacnt_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS_eystrgacnt'"
		},
		"LS_azuresqldatabase_userdb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_azuresqldatabase_userdb'"
		},
		"LS_new_for demo_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_new_for demo'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://eystrgacnt.dfs.core.windows.net"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://demo-keyvault-ey.vault.azure.net/"
		},
		"LS_ADLS_eystrgacnt_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://eystrgacnt.dfs.core.windows.net/"
		},
		"LS_new_for demo_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://eystrgacnt.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfneww1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_eystrgacnt_accountKey": {
						"value": "[parameters('LS_ADLS_eystrgacnt_accountKey')]"
					},
					"LS_azuresqldatabase_userdb_connectionString": {
						"value": "[parameters('LS_azuresqldatabase_userdb_connectionString')]"
					},
					"LS_new_for demo_accountKey": {
						"value": "[parameters('LS_new_for demo_accountKey')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"LS_ADLS_eystrgacnt_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_eystrgacnt_properties_typeProperties_url')]"
					},
					"LS_new_for demo_properties_typeProperties_url": {
						"value": "[parameters('LS_new_for demo_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}